    "stop-xampp": "cmd /C taskkill /F /IM httpd.exe",
    "stop-mysql": "cmd /C taskkill /F /IM mysqld.exe",
    "stop": "npx concurrently \"npm run stop-xampp\" \"npm run stop-mysql\"",
