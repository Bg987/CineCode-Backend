  "scripts": {
    "start-xampp": "start /B C:\\xampp\\xampp_start.exe > xampp.log 2>&1",
    "start-mysql": "start /B C:\\xampp\\mysql\\bin\\mysqld.exe > mysql.log 2>&1",
